{
    "name": "rych/otp",
    "type": "library",
    "description": "PHP implementation of the OATH one-time password standards",
    "keywords": ["security", "otp", "hotp", "totp", "oath", "rfc4226", "rfc6238"],
    "homepage": "https://github.com/rchouinard/rych-otp",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Chouinard",
            "email": "rchouinard@gmail.com",
            "homepage": "https://www.ryanchouinard.com/"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "ext-hash": "*",
        "paragonie/random_compat": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0",
        "squizlabs/php_codesniffer": "^2.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rych\\Otp\\": "src/"
        },
        "files": [
            "src/functions/utility_functions.php",
            "src/functions/base32_functions.php",
            "src/functions/otp_functions.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "2.0-dev"
        }
    }
}
